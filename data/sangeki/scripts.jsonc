          "numberOfLoops": 4,
          "difficulty": 6
        }
      ],
      "tragedySet": "basicTragedy",
      "mainPlot": [
        "giantTimeBomb"
      ],
      "subPlots": [
        "threadsFate",
        "circleFriends"
      ],
      "daysPerLoop": 7,
      "cast": {
        "henchman": "witch",
        "godlyBeing": [
          "friend",
          {
            "enters on loop": 3
          }
        ],
        "littleSister": "friend",
        "policeOfficer": "conspiracyTheorist",
